{
  "name": "monorepo-template-native",
  "descrpition": "Turborepo template with react native and NativeBase support.",
  "author": {
    "name": "Abderraouf Zine",
    "email": "rofazayn@gmail.com",
    "url": "https://rofazayn.com"
  },
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "clean": "turbo run clean && rm -rf node_modules",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\" --ignore-path .gitignore"
  },
  "devDependencies": {
    "prettier": "^2.7.1",
    "turbo": "latest"
  }
}
